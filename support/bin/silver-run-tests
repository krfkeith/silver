#!/bin/bash

if [ -z "$SILVER_HOME" ]; then
  echo "SILVER_HOME must be set to run this prototype test harness."
  exit 1
fi

TestJar=$SILVER_HOME/../grammars/lib/testing/lib.testing.jar

if [ ! -f $TestJar ]; then
  echo "Could not find prototype test harness jar file."
  exit 1
fi

java -jar $TestJar $@
