<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="@GROUP_ID@.feature"
      label="@LANG_NAME@ Eclipse feature"
      version="@IDE_VERSION@.@IDE_BUILD_TIMESTAMP@">

   <description url="@FEATURE_DESCRIPTION_URL@">
      @FEATURE_DESCRIPTION_TEXT@
   </description>

   <copyright url="@FEATURE_COPYRIGHT_URL@">
      @FEATURE_COPYRIGHT_TEXT@
   </copyright>

   <license url="@FEATURE_LICENSE_URL@">
      @FEATURE_LICENSE_TEXT@
   </license>

   <!-- The IDE plugin's id is the symbolic name in manifest file -->
   <plugin
         id="@GROUP_ID@"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="true"/>
         
   <plugin
         id="org.eclipse.imp.runtime"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="lpg.runtime"
         download-size="0"
         install-size="0"
         version="2.0.18"
         unpack="false"/>

   <plugin
         id="lpg.runtime.java"
         download-size="0"
         install-size="0"
         version="2.0.18"
         unpack="false"/>

</feature>
